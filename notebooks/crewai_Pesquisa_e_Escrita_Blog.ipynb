      "source": [
        "# ü§ñ Pesquisa e Escrita Blog - crewai ü§ñ\n",
        "\n",
        "!pip install -q crewai crewai_tools\n",
        "!pip install langchain\n",
        "\n",
        "import os\n",
        "from crewai import Agent, Task, Crew\n",
        "from crewai_tools import SerperDevTool\n",
        "\n",
        "# Importe a biblioteca userdata do google.colab\n",
        "from google.colab import userdata\n",
        "\n",
       # Configurar as chaves de API
        "os.environ[\"OPENAI_API_KEY"] = "sua_chave_openai"
        "os.environ[\"OPENAI_MODEL_NAME"] = "gpt-4"
        "os.environ[\"SERPER_API_KEY"] = "sua_chave_serper"
        "\n",
        "# Definir a ferramenta de pesquisa\n",
        "search_tool = SerperDevTool()\n",
        "\n",
        "# Definir o agente de pesquisa\n",
        "research_agent = Agent(\n",
        "    role='Pesquisador',\n",
        "    goal='Encontrar e sumarizar as √∫ltimas not√≠cias de IA',\n",
        "    backstory=\"\"\"Voc√™ √© um pesquisador em uma grande empresa.\n",
        "    Voc√™ √© respons√°vel por analisar dados e fornecer insights para o neg√≥cio.\"\"\",\n",
        "    verbose=True,\n",
        "    tools=[search_tool]\n",
        ")\n",
        "\n",
        "# Definir o agente de escrita\n",
        "writer_agent = Agent(\n",
        "    role='Escritor',\n",
        "    goal='Escrever artigos e posts de blog envolventes',\n",
        "    backstory=\"\"\"Voc√™ √© um escritor experiente em uma grande empresa.\n",
        "    Voc√™ √© respons√°vel por criar conte√∫do envolvente e informativo.\"\"\",\n",
        "    verbose=True\n",
        ")\n",
        "\n",
        "# Tarefa: Pesquisar not√≠cias de IA\n",
        "research_ai_task = Task(\n",
        "    description='Encontrar e sumarizar as √∫ltimas not√≠cias de IA',\n",
        "    expected_output='Uma lista com marcadores do sum√°rio das 5 not√≠cias mais importantes de IA',\n",
        "    agent=research_agent,\n",
        "    tools=[search_tool],\n",
        "    async_execution=True\n",
        ")\n",
        "\n",
        "# Tarefa: Pesquisar not√≠cias de Opera√ß√µes de IA\n",
        "research_ops_task = Task(\n",
        "    description='Encontrar e sumarizar as √∫ltimas not√≠cias de Opera√ß√µes de IA',\n",
        "    expected_output='Uma lista com marcadores do sum√°rio das 5 not√≠cias mais importantes de Opera√ß√µes de IA',\n",
        "    agent=research_agent,\n",
        "    tools=[search_tool],\n",
        "    async_execution=True\n",
        ")\n",
        "\n",
        "# Tarefa: Escrever um post de blog\n",
        "write_blog_task = Task(\n",
        "    description='Escreva um post de blog completo sobre a import√¢ncia da IA e suas √∫ltimas not√≠cias',\n",
        "    expected_output='Post de blog completo com 4 par√°grafos de extens√£o',\n",
        "    agent=writer_agent,\n",
        "    context=[research_ai_task, research_ops_task]\n",
        ")\n",
        "\n",
        "# Tarefa: Listar ideias para um artigo sobre IA\n",
        "list_ideas = Task(\n",
        "    description=\"Liste 5 ideias interessantes para explorar para um artigo sobre IA.\",\n",
        "    expected_output=\"Lista com marcadores de 5 ideias para um artigo.\",\n",
        "    agent=research_agent,\n",
        "    async_execution=True  # Ser√° executado de forma ass√≠ncrona\n",
        ")\n",
        "\n",
        "# Tarefa: Listar eventos importantes da hist√≥ria da IA\n",
        "list_important_history = Task(\n",
        "    description=\"Pesquise a hist√≥ria da IA e me d√™ os 5 eventos mais importantes.\",\n",
        "    expected_output=\"Lista com marcadores de 5 eventos importantes.\",\n",
        "    agent=research_agent,\n",
        "    async_execution=True  # Ser√° executado de forma ass√≠ncrona\n",
        ")\n",
        "\n",
        "# Tarefa: Escrever um artigo sobre IA\n",
        "write_article = Task(\n",
        "    description=\"Escreva um artigo sobre IA, sua hist√≥ria e ideias interessantes.\",\n",
        "    expected_output=\"Um artigo de 4 par√°grafos sobre IA.\",\n",
        "    agent=writer_agent,\n",
        "    context=[list_ideas, list_important_history]  # Vai esperar pela sa√≠da das duas tarefas\n",
        ")\n",
        "\n",
        "# Fun√ß√£o de callback\n",
        "def callback_function(output):\n",
        "    \"\"\"\n",
        "    Faz algo depois que a tarefa √© completada.\n",
        "    \"\"\"\n",
        "    print(f\"Tarefa completada: {output.description}\")\n",
        "    if hasattr(output, 'output'):\n",
        "        print(f\"Sa√≠da: {output.output}\")\n",
        "    else:\n",
        "        print(f\"Atributos dispon√≠veis: {dir(output)}\")\n",
        "\n",
        "# Tarefa com callback\n",
        "research_task_with_callback = Task(\n",
        "    description='Encontrar e sumarizar as √∫ltimas not√≠cias de IA',\n",
        "    expected_output='Uma lista com marcadores do sum√°rio das 5 not√≠cias mais importantes de IA',\n",
        "    agent=research_agent,\n",
        "    tools=[search_tool],\n",
        "    callback=callback_function\n",
        ")\n",
        "\n",
        "# Criar a equipe\n",
        "crew = Crew(\n",
        "    agents=[research_agent, writer_agent],\n",
        "    tasks=[research_ai_task, research_ops_task, write_blog_task,\n",
        "           list_ideas, list_important_history, write_article,\n",
        "           research_task_with_callback],\n",
        "    verbose=True\n",
        ")\n",
        "\n",
        "# Executar a equipe\n",
        "result = crew.kickoff()\n",
        "\n",
        "# Inspecionar e imprimir o resultado\n",
        "print(\"Resultado retornado por `crew.kickoff()`:\", result)\n",
        "print(\"Atributos dispon√≠veis:\", dir(result))\n",
        "\n",
        "if isinstance(result, list):  # Caso o resultado seja uma lista de tarefas\n",
        "    for task_result in result:\n",
        "        print(f\"Tarefa: {task_result.description}\")\n",
        "        if hasattr(task_result, 'output'):\n",
        "            print(f\"Sa√≠da: {task_result.output}\")\n",
        "        else:\n",
        "            print(f\"Atributos dispon√≠veis: {dir(task_result)}\")\n",
        "else:\n",
        "    print(\"Resultado inesperado. Estrutura do objeto:\", type(result))\n"
      ]
    }
  ]
